generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @db.VarChar(40)
  password   String   @db.VarChar(40)
  created_at DateTime @default(now()) @db.Date
}

model Teste {
  id         Int      @id @default(autoincrement())
  email      String   @db.VarChar(40)
  password   String   @db.VarChar(40)
  created_at DateTime @default(now()) @db.Date
}
